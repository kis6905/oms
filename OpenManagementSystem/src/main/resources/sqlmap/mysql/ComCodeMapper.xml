<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.open.ms.mapper.common.ComCodeMapper">

    <select id="getComCode" parameterType="com.open.ms.vo.common.ComCode" resultType="com.open.ms.vo.common.ComCode">
        SELECT 
			codeGroup,
			code,
			codeTitle,
			codeValue
		FROM oms_com_code
		WHERE codeGroup = #{codeGroup}
		  AND code = #{code}
    </select>
    
</mapper>
